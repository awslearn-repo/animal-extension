{
  "manifest_version": 3,
  "name": "Animal Explorer",
  "version": "1.0.0",
  "description": "Explore animals with images, sounds, and categories.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Animal Explorer"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "icons/*",
        "css/*",
        "js/*",
        "data/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}

