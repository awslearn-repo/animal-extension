{
  "manifest_version": 3,
  "name": "Animal Sounds & Facts",
  "version": "1.0.0",
  "description": "Browse animal categories, see details, and play sounds in a clean popup UI.",
  "action": {
    "default_title": "Animal Sounds & Facts",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
